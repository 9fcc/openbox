include build/Makefile.incl

all clean distclean:
	@$(MAKE) -$(MAKEFLAGS) -f build/Makefile.engines.openbox $@

install:
	@$(MAKE) -$(MAKEFLAGS) -f build/Makefile.engines.openbox $@
	$(LIBTOOL) --mode=finish $(DESTDIR)$(enginedir)

uninstall:
	@$(MAKE) -$(MAKEFLAGS) -f build/Makefile.engines.openbox $@
	-rmdir $(DESTDIR)$(enginedir)

.PHONY: all clean distclean install uninstall
