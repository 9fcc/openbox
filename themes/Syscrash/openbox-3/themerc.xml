    <inactive>
      <primary r="34" g="34" b="28" a="255"/>
    </inactive>
    <disabled>
      <primary r="140" g="140" b="117" a="255"/>
    </disabled>
    <activedisabled>
      <text>
        <primary r="140" g="140" b="117" a="255"/>
      </text>
      <style>flat solid</style>
      <primary r="76" g="76" b="76" a="255"/>
    </activedisabled>
    <active>
      <text>
        <primary r="255" g="255" b="255" a="255"/>
      </text>
      <style>flat solid</style>
      <primary r="76" g="76" b="76" a="255"/>
    </active>
    <entries>
      <style>flat solid</style>
      <primary r="230" g="230" b="224" a="255"/>
    </entries>
  </menu>
